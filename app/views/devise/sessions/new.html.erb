<div class="login-btn-row">
  <%= link_to "Log In", new_user_session_path, class: "realtalk-btn btn btn-primary btn-lg" %>
  <%= link_to "Sign Up", new_user_registration_path, class: "realtalk-btn btn btn-primary btn-lg inactive" %>
</div>
<br>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.email_field :email, placeholder: "Email", autofocus: true %>
  </div>

  <div class="field">
    <%= f.password_field :password, placeholder:"Password", autocomplete: "off" %>
  </div>
  <div>
  <span class="pull-right">
  <%= link_to "Can't log in?", new_password_path(resource_name) %><br />
  </span>
  <span class="pull-left">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
  </span>
  <div class="wrapper clear">
    <div style="clear: both;"></div>
</div>
  </div>
  <div class="actions">
    <%= f.submit "Log in", class: "realtalk-btn btn btn-primary btn-lg" %>
  </div>
<% end %>
